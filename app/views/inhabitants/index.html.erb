<h1>Listing inhabitants and cities</h1>
<table>
  <tr>
    <th>Username</th>
    <th>Favs</th>
    <th>Url</th>
  </tr>

<% for inhabitant in @inhabitants %>
  <tr>
    <td><%= link_to (inhabitant.login || "without username yet"), inhabitant %></td>
    <td><%=h inhabitant.favs %></td>
    <%url=inhabitant.url%>
    <td><%=link_to(url, url) if url%>
  </tr>
<% end %>
</table>

